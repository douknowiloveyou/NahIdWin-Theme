  "semanticHighlighting": true,
  "semanticTokenColors": {
    // PYTHON
    "class.decorator.builtin": {
      "foreground": "#78A9FF"
    },
    "class.typeHint": {
      "foreground": "#787878"
    },
    "module.typeHint": {
      "foreground": "#787878"
    },
    "builtinConstant.typeHint": {
      "foreground": "#787878"
    },
    "enum.typeHint": {
      "foreground": "#787878"
    },

    // TYPESCRIPT
    "function.declaration": {
      "foreground": "#FF7EB6"
    }
  },

  "tokenColors": [
    //////////////////////
    ////// INDENT
    //////////////////////
    {
      "scope": [],
      "settings": {
        "fontStyle": "italic"
      }
    },

    //////////////////////
    ////// RED
    //////////////////////
    {
      "scope": [
        // PYTHON
        "entity.name.type.class.python",
        "support.function.magic.python",
        "entity.name.function.python",

        // CSS
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",

        // SVELTE
        "entity.name.tag.svelte"
      ],
      "settings": {
        "foreground": "#FF7EB6",
        "fontStyle": "bold"
      }
    },

    //////////////////////
    ////// BLUE
    //////////////////////
    {
      "scope": [
        // PYTHON
        "support.function.builtin.python",
        "meta.function-call.generic.python",
        "support.function.misc.css",

        // CSS
        "support.function.calc.css",

        // TYPESCRIPT
        "entity.name.function.ts"
      ],
      "settings": {
        "foreground": "#B2CFFF",
        "fontStyle": "bold"
      }
    },

    //////////////////////
    ////// DARK BLUE
    //////////////////////
    {
      "scope": [
        // PYTHON
        "storage.type.function.python",
        "storage.type.function.async.python",
        "storage.type.class.python",

        // TYPESCRIPT
        "storage.type.function.ts"
      ],
      "settings": {
        "foreground": "#82CFFF",
        "fontStyle": "bold"
      }
    },

    //////////////////////
    ////// DARKEST BLUE
    //////////////////////
    {
      "scope": [
        // PYTHON
        "entity.name.function.decorator.python",
        "keyword.control.import.python",
        "keyword.control.from.ts",

        // TYPESCRIPT
        "keyword.control.import.ts",
        "keyword.control.export.ts"
      ],
      "settings": {
        "foreground": "#78A9FF",
        "fontStyle": "bold"
      }
    },

    //////////////////////
    ////// PURPLE
    //////////////////////
    {
      "scope": [
        // PYTHON
        "variable.parameter.function.language.python",

        // CSS
        "support.type.property-name.css",

        // TYPESCRIPT
        "variable.parameter.ts",

        // SVELTE
        "entity.other.attribute-name.svelte"
      ],
      "settings": {
        "foreground": "#BE95FF",
        "fontStyle": "bold"
      }
    },

    //////////////////////
    ////// GREEN
    //////////////////////
    {
      "scope": [
        // PYTHON
        "string.quoted.single.python",
        "string.quoted.multi.python",
        "constant.numeric.dec.python",
        "constant.numeric.float.python",
        "constant.language.python",

        // JSON
        "string.quoted.double.json.comments",
        "constant.numeric.json.comments",
        "constant.language.json.comments",

        // CSS
        // "constant.numeric.css",

        // TYPESCRIPT
        "new.expr.ts",
        "constant.numeric.decimal.ts",

        // SVELTE
        "string.quoted.svelte"
      ],
      "settings": {
        "foreground": "#85D0B7",
        "fontStyle": "bold"
      }
    },
    {
      "scope": [
        // PYTHON
      ],
      "settings": {
        "foreground": "#85D0B7",
        "fontStyle": "italic"
      }
    },

    //////////////////////
    ////// YELLOW
    //////////////////////
    {
      "scope": [
        // PYTHON
        "keyword.control.flow.python",
        "keyword.operator.logical.python",

        // TYPESCRIPT
        "storage.type.ts",
        "keyword.control.flow.ts",
        "keyword.operator.new.ts",
        "punctuation.definition.variable.svelte"
      ],
      "settings": {
        "foreground": "#D3C6AA",
        "fontStyle": "bold"
      }
    },

    //////////////////////
    ////// PINK
    //////////////////////
    {
      "scope": [
        // PYTHON
        "punctuation.separator.annotation.result.python",
        "keyword.operator.assignment.python",
        "keyword.operator.arithmetic.python",
        "keyword.operator.bitwise.python",
        "keyword.operator.comparison.python",
        "keyword.operator.unpacking.arguments.python"
      ],
      "settings": {
        "foreground": "#FFB6C1",
        "fontStyle": "bold"
      }
    },

    //////////////////////
    ////// ORANGE
    //////////////////////
    {
      "scope": [],
      "settings": {
        "foreground": "#E69875"
      }
    },

    //////////////////////
    ////// TYPEHINT GRAY
    //////////////////////
    {
      "scope": [
        // TYPESCRIPT
        "text.svelte",
        "support.type.primitive.ts"
      ],
      "settings": {
        "foreground": "#787878"
      }
    },

    //////////////////////
    ////// FOREGROUND GRAY
    //////////////////////
    {
      "scope": [
        // JSON
        "support.type.property-name.json.comments",

        // TYPESCRIPT
        "variable.other.object.ts",
        "variable.other.readwrite.ts"
      ],
      "settings": {
        "foreground": "#B1B6B9"
      }
    },

    //////////////////////
    ////// COMMENT/PUNCTUATION GRAY
    //////////////////////
    {
      "scope": [
        // PYTHON
        "punctuation.definition.arguments.begin.python",
        "punctuation.definition.arguments.end.python",
        "punctuation.definition.list.begin.python",
        "punctuation.definition.list.end.python",
        "punctuation.parenthesis.begin.python",
        "punctuation.parenthesis.end.python",
        "string.quoted.docstring.multi.python",
        "punctuation.separator.annotation.result.python",
        "keyword.operator.assignment.python",
        "keyword.operator.arithmetic.python",
        "keyword.operator.bitwise.python",
        "keyword.operator.comparison.python",
        "keyword.operator.unpacking.arguments.python",
        "punctuation.definition.string.begin.python",
        "punctuation.definition.string.end.python",
        "variable.parameter.function-call.python",

        // JSON
        "punctuation.definition.array.begin.json",
        "punctuation.definition.array.end.json",
        "punctuation.definition.dictionary.begin.json",
        "punctuation.definition.dictionary.end.json",
        "punctuation.definition.string.begin.json",
        "punctuation.definition.string.end.json",
        "punctuation.separator.array.json",
        "punctuation.separator.dictionary.key-value.json",
        "punctuation.separator.dictionary.pair.json",
        "punctuation.support.type.property-name.begin.json",
        "punctuation.support.type.property-name.end.json",
        "constant.language.json.comments",

        // CSS
        "punctuation.section.property-list.begin.bracket.curly.css",
        "punctuation.section.property-list.end.bracket.curly.css",
        "punctuation.section.function.begin.bracket.round.css",
        "punctuation.section.function.end.bracket.round.css",

        // TYPESCRIPT
        "comment.line.double-slash.ts",
        "meta.brace.round.ts",
        "keyword.operator.arithmetic.ts",
        "punctuation.definition.block.ts",
        "punctuation.definition.parameters.begin.ts",
        "punctuation.definition.parameters.end.ts",
        "keyword.operator.assignment.compound.ts",
        "storage.type.function.arrow.ts",
        "comment.block.documentation.ts",

        // SVELTE
        "keyword.operator.comparison.ts",
        "punctuation.definition.tag.begin.svelte",
        "punctuation.definition.tag.end.svelte",
        "punctuation.definition.string.begin.svelte",
        "punctuation.definition.string.end.svelte",
        "punctuation.section.embedded.begin.svelte",
        "punctuation.section.embedded.end.svelte"
      ],
      "settings": {
        "foreground": "#637179"
      }
    },
    {
      "scope": [
        // PYTHON
        "comment.line.number-sign.python",
        "comment.line.double-slash.js"
      ],
      "settings": {
        "foreground": "#637179",
        "fontStyle": "italic"
      }
    }
  ]
}
